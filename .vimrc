syntax on
set tabstop=2
set shiftwidth=2
set expandtab
set ai
set number
set hlsearch
set ruler

filetype on
"filetype plugin on
filetype indent on

colorscheme peachpuff
hi Comment ctermfg=green
hi Search cterm=NONE ctermfg=black ctermbg=grey
hi Visual cterm=NONE ctermfg=black ctermbg=grey

set title
set laststatus=2

